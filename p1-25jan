using namespace std;
int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
    int arr[4];
    for(int i=0;i<4;i++)
    {
        cin>>arr[i];
    }
    int i=0;
    int sum=0;
    for( i=0;i<3;i++)
    {
        cout<<"P"<<i+1<<" "<<"("<<"WT="<<sum<<")"<<", ";
        sum+=arr[i];
    }
    cout<<"P"<<i+1<<" "<<"("<<"WT="<<sum<<")";
    return 0;
}
